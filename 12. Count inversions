//Brute force approach

#include <bits/stdc++.h> 
long long getInversions(long long *arr, int n){
     int ct=0;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(arr[i]>arr[j] && i<j)
              ct++;
        }
    }
    return ct;
}

